        TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
        TweenService:Create(SettingsStroke, TweenInfo.new(0.3), {Transparency = 0.2}):Play()
    end)
    
    SettingsButton.MouseLeave:Connect(function()
        if not SettingsFrame.Visible then
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {BackgroundColor3 = Color3.fromRGB(30, 30, 38)}):Play()
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {ImageColor3 = Color3.fromRGB(160, 160, 180)}):Play()
            TweenService:Create(SettingsStroke, TweenInfo.new(0.3), {Transparency = 0.6}):Play()
        end
    end)
    
    SettingsButton.MouseButton1Click:Connect(function()
        SettingsFrame.Visible = not SettingsFrame.Visible
        if SettingsFrame.Visible then
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {BackgroundColor3 = Color3.fromRGB(60, 60, 85)}):Play()
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {ImageColor3 = Color3.fromRGB(255, 255, 255)}):Play()
            TweenService:Create(SettingsStroke, TweenInfo.new(0.3), {Transparency = 0.1}):Play()
        else
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {BackgroundColor3 = Color3.fromRGB(30, 30, 38)}):Play()
            TweenService:Create(SettingsButton, TweenInfo.new(0.3, Enum.EasingStyle.Quart), {ImageColor3 = Color3.fromRGB(160, 160, 180)}):Play()
            TweenService:Create(SettingsStroke, TweenInfo.new(0.3), {Transparency = 0.6}):Play()
        end
    end)
    
    local UIToggle = Instance.new("Frame")
