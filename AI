    local AutoLoadCircle = Instance.new("Frame")
    AutoLoadCircle.Size = UDim2.new(0, 18, 0, 18)
    AutoLoadCircle.Position = UDim2.new(0, 3, 0.5, -9)
    AutoLoadCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    AutoLoadCircle.BorderSizePixel = 0
    AutoLoadCircle.Parent = AutoLoadSwitch
    
    local AutoLoadCircleCorner = Instance.new("UICorner")
    AutoLoadCircleCorner.CornerRadius = UDim.new(1, 0)
    AutoLoadCircleCorner.Parent = AutoLoadCircle
    
    local AutoLoadButton = Instance.new("TextButton")
    AutoLoadButton.Size = UDim2.new(1, 0, 1, 0)
    AutoLoadButton.BackgroundTransparency = 1
    AutoLoadButton.Text = ""
    AutoLoadButton.Parent = AutoLoadToggle
    
    AutoLoadButton.MouseButton1Click:Connect(function()
        GUI.Settings.AutoLoad = not GUI.Settings.AutoLoad
        TweenService:Create(AutoLoadSwitch, TweenInfo.new(0.2), {BackgroundColor3 = GUI.Settings.AutoLoad and Color3.fromRGB(60, 180, 60) or Color3.fromRGB(50, 50, 50)}):Play()
        TweenService:Create(AutoLoadCircle, TweenInfo.new(0.2), {Position = GUI.Settings.AutoLoad and UDim2.new(1, -21, 0.5, -9) or UDim2.new(0, 3, 0.5, -9)}):Play()
    end)
    
    SettingsTab.Frame = SettingsFrame
    SettingsTab.Container = SettingsContainer
    SettingsTab.Button = SettingsButton
    SettingsTab.ButtonStroke = SettingsStroke
    SettingsTab.TopBar = SettingsTopBar
    SettingsTab.FrameStroke = SettingsFrameStroke
    
    table.insert(GUI.Tabs, SettingsTab)
    
    return GUI
end

return ZOV
