    local ThemeOptionsList = Instance.new("Frame")
    ThemeOptionsList.Size = UDim2.new(1, 0, 0, 0)
    ThemeOptionsList.Position = UDim2.new(0, 0, 0, 40)
    ThemeOptionsList.BackgroundTransparency = 1
    ThemeOptionsList.Parent = ThemeDropdown
    
    local ThemeOptionsLayout = Instance.new("UIListLayout")
    ThemeOptionsLayout.Parent = ThemeOptionsList
    
    local themeExpanded = false
    local themeOptions = {"Dark", "Blue", "Purple", "Red"}
    
    for _, themeName in ipairs(themeOptions) do
        local ThemeOption = Instance.new("TextButton")
        ThemeOption.Size = UDim2.new(1, 0, 0, 35)
        ThemeOption.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
        ThemeOption.Text = themeName
        ThemeOption.TextColor3 = Color3.fromRGB(255, 255, 255)
        ThemeOption.TextSize = 14
        ThemeOption.Font = Enum.Font.Gotham
        ThemeOption.BorderSizePixel = 0
        ThemeOption.Parent = ThemeOptionsList
        
        ThemeOption.MouseButton1Click:Connect(function()
            ThemeLabel.Text = "Theme: " .. themeName
            GUI:ApplyTheme(themeName)
            themeExpanded = false
            TweenService:Create(ThemeDropdown, TweenInfo.new(0.2), {Size = UDim2.new(1, 0, 0, 40)}):Play()
            ThemeArrow.Text = "▼"
        end)
    end
    
    local ThemeButton = Instance.new("TextButton")
    ThemeButton.Size = UDim2.new(1, 0, 0, 40)
    ThemeButton.BackgroundTransparency = 1
    ThemeButton.Text = ""
    ThemeButton.ZIndex = 2
    ThemeButton.Parent = ThemeDropdown
    
    ThemeButton.MouseButton1Click:Connect(function()
        themeExpanded = not themeExpanded
        local newSize = themeExpanded and (40 + #themeOptions * 35) or 40
        TweenService:Create(ThemeDropdown, TweenInfo.new(0.2), {Size = UDim2.new(1, 0, 0, newSize)}):Play()
        ThemeArrow.Text = themeExpanded and "▲" or "▼"
    end)
    
    local AutoLoadToggle = Instance.new("Frame")
    AutoLoadToggle.Size = UDim2.new(1, 0, 0, 40)
    AutoLoadToggle.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    AutoLoadToggle.BorderSizePixel = 0
    AutoLoadToggle.Parent = SettingsContainer
    
    local AutoLoadCorner = Instance.new("UICorner")
