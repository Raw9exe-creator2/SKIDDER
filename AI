        TweenService:Create(SettingsCloseButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 50)}):Play()
    end)
    
    SettingsCloseButton.MouseButton1Click:Connect(function()
        SettingsFrame.Visible = false
        TweenService:Create(SettingsButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 30, 38)}):Play()
        TweenService:Create(SettingsButton, TweenInfo.new(0.2), {ImageColor3 = Color3.fromRGB(160, 160, 180)}):Play()
        TweenService:Create(SettingsStroke, TweenInfo.new(0.2), {Transparency = 0.6}):Play()
    end)
    
